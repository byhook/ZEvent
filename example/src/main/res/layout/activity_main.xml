<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="cn.byhook.dimensmaker.MainActivity">


    <TextView
        android:id="@+id/main_tv_info"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:lineSpacingExtra="@dimen/px_10"
        android:padding="@dimen/px_20"
        android:text="@string/screen_notify_info" />

    <TextView
        android:layout_width="@dimen/px_375"
        android:layout_height="@dimen/px_100"
        android:layout_below="@id/main_tv_info"
        android:layout_marginTop="@dimen/px_100"
        android:background="@color/colorAccent" />

</RelativeLayout>
